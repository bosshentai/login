<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>


<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="com.mycompany.login.PrimaryController">
   <children>
      <Label text="Email" textFill="green"  />
      <javafx.scene.control.TextField fx:id="email" promptText="O seu email" />
      <Label text="Password"/>
      <javafx.scene.control.PasswordField fx:id="password" promptText="a sua password"  />
     
          <Button fx:id="primaryButton" text="Login" onAction="#loginTest" />
      <Button fx:id="registerButton" text="Registar" onAction="#switchToSecondary"/> 
      
     
      
  
          <ListView fx:id="personaInfo">   
          </ListView> 
      
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
